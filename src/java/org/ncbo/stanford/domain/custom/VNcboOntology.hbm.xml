<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    Custom mapping file for NcboOntology
-->
<hibernate-mapping>
    <class name="org.ncbo.stanford.domain.custom.entity.VNcboOntology" table="v_ncbo_ontology" catalog="bioportal">          
		<id name="id" type="java.lang.Integer">
		    <column name="id" />
		</id>           
		<property name="ontologyId" type="java.lang.Integer">
		    <column name="ontology_id" />
		</property>
		<property name="userId" type="java.lang.Integer">
		    <column name="user_id" />
		</property>
		<property name="internalVersionNumber" type="java.lang.Integer">
		    <column name="internal_version_number" />
		</property>
		<property name="versionNumber" type="java.lang.String">
		    <column name="version_number" length="64" />
		</property>
		<property name="versionStatus" type="java.lang.String">
		    <column name="version_status" length="64" />
		</property>
		<property name="filePath" type="java.lang.String">
		    <column name="file_path" length="2048" />
		</property>
		<property name="isRemote" type="java.lang.Byte">
		    <column name="is_remote" />
		</property>
		<property name="isReviewed" type="java.lang.Byte">
		    <column name="is_reviewed" />
		</property>
		<property name="statusId" type="java.lang.Integer">
		    <column name="status_id" />
		</property>
		<property name="dateCreated" type="java.util.Date">
		    <column name="date_created" length="0" />
		</property>
		<property name="dateReleased" type="java.util.Date">
		    <column name="date_released" length="0" />
		</property>
		<property name="oboFoundryId" type="java.lang.String">
		    <column name="obo_foundry_id" length="128" />
		</property>
		<property name="isManual" type="java.lang.Byte">
		    <column name="is_manual" />
		</property>
		<property name="displayLabel" type="java.lang.String">
		    <column name="display_label" length="128" />
		</property>		
		<property name="description" type="java.lang.String">
		    <column name="description" length="512" />
		</property>
		<property name="abbreviation" type="java.lang.String">
		    <column name="abbreviation" length="32" />
		</property>
		<property name="format" type="java.lang.String">
		    <column name="format" length="50" />
		</property>
		<property name="contactName" type="java.lang.String">
		    <column name="contact_name" length="128" />
		</property>
		<property name="contactEmail" type="java.lang.String">
		    <column name="contact_email" length="128" />
		</property>
		<property name="homepage" type="java.lang.String">
		    <column name="homepage" length="2048" />
		</property>
		<property name="documentation" type="java.lang.String">
		    <column name="documentation" length="2048" />
		</property>
		<property name="publication" type="java.lang.String">
		    <column name="publication" length="2048" />
		</property>
		<property name="urn" type="java.lang.String">
		    <column name="urn" length="512" />
		</property>
		<property name="codingScheme" type="java.lang.String">
		    <column name="coding_scheme" length="256" />
		</property>
		<property name="targetTerminologies" type="java.lang.String">
		    <column name="target_terminologies" length="256" />
		</property>
		<property name="isFoundry" type="java.lang.Byte">
		    <column name="is_foundry" />
		</property>		
		<property name="synonymSlot" type="java.lang.String">
		    <column name="synonym_slot" length="512" />
		</property>
		<property name="preferredNameSlot" type="java.lang.String">
		    <column name="preferred_name_slot" length="512" />
		</property>
        <set name="ncboOntologyCategories" inverse="true">
            <key>
                <column name="ontology_version_id" not-null="true" />
            </key>
            <one-to-many class="org.ncbo.stanford.domain.generated.NcboOntologyCategory" />
        </set>
        <set name="ncboOntologyFiles" inverse="true">
            <key>
                <column name="ontology_version_id" not-null="true" />
            </key>
            <one-to-many class="org.ncbo.stanford.domain.generated.NcboOntologyFile" />
        </set>
	</class>
</hibernate-mapping>
